        $('.chList:visible').prop('checked', true).each(function() {
            var row = document.getElementsByClassName($(this).val())[0];
            var style = row.getAttribute("style");
            row.style.backgroundColor = "#66FF66";
            check_arr.push($(this).val());
            check_style.push(style);
			console.log(check_arr)
			console.log(check_style)
        });
    } else {
        $('.chList:visible').prop('checked', false).each(function() {
            var row = document.getElementsByClassName($(this).val())[0];
            var index = check_arr.indexOf($(this).val());
            row.setAttribute('style', check_style[index]);
            check_arr.splice(index, 1);
            check_style.splice(index, 1);
        });
    }
});
		
	
	
